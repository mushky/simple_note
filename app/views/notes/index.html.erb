<p><%= link_to "Add Note", new_note_path, class: "btn-sm btn-primary" %></p>

<% @notes.each do |note| %>
	<div class="card">
		<div class="card-body">
			<%= link_to note.title, edit_note_path(note) %><br>
			<%= sanitize note.description %>
		</div> <!-- /card-body -->
	</div> <!-- /card -->
<% end %>

